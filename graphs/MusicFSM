digraph finite_state_machine {
	rankdir=LR
	node [shape = circle]
	IDLE -> IDLE
	IDLE -> SPOTIFY_OPEN
	SPOTIFY_OPEN -> SPOTIFY_PAUSED
	NEW_SONG -> SPOTIFY_PLAYING
	SPOTIFY_PLAYING -> SPOTIFY_PLAYING
	SPOTIFY_PLAYING -> NEW_SONG
	SPOTIFY_PLAYING -> SPOTIFY_PAUSED
	SPOTIFY_PAUSED -> SHUTDOWN
	SPOTIFY_PAUSED -> NEW_SONG
	SHUTDOWN -> IDLE
	
}